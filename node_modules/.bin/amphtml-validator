#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tienlx97/work/ct/next.js/node_modules/.pnpm/amphtml-validator@1.0.35/node_modules/amphtml-validator/node_modules:/home/tienlx97/work/ct/next.js/node_modules/.pnpm/amphtml-validator@1.0.35/node_modules:/home/tienlx97/work/ct/next.js/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tienlx97/work/ct/next.js/node_modules/.pnpm/amphtml-validator@1.0.35/node_modules/amphtml-validator/node_modules:/home/tienlx97/work/ct/next.js/node_modules/.pnpm/amphtml-validator@1.0.35/node_modules:/home/tienlx97/work/ct/next.js/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../amphtml-validator/cli.js" "$@"
else
  exec node  "$basedir/../amphtml-validator/cli.js" "$@"
fi
